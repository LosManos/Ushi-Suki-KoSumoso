{
    "name": "try-cosmos-query-tool",
    "private": true,
    "version": "0.0.0",
    "main": "dist-electron/main.js",
    "scripts": {
        "dev": "concurrently \"tsc -w -p tsconfig.electron.json\" \"vite\" \"wait-on tcp:5173 && cross-env VITE_DEV_SERVER_URL=http://localhost:5173 electron .\"",
        "build": "tsc -p tsconfig.electron.json && vite build && electron-builder",
        "preview": "vite preview"
    },
    "dependencies": {
        "@azure/cosmos": "^4.0.0",
        "@azure/identity": "^4.13.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@types/react": "^18.2.66",
        "@types/react-dom": "^18.2.22",
        "@vitejs/plugin-react": "^4.2.1",
        "concurrently": "^8.2.2",
        "cross-env": "^7.0.3",
        "electron": "^30.0.0",
        "electron-builder": "^24.13.3",
        "typescript": "^5.2.2",
        "vite": "^5.2.0",
        "wait-on": "^7.2.0"
    }
}
